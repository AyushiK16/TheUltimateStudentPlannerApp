{
  "expo": {
    "name": "The Ultimate Student Planner App",
    "slug": "C77",
    "version": "1.0.0",
    "orientation": "portrait",
    "description": "This is an app meant for helping students in preparing for their exams, by keeping track of reminders, syllabus and marks!",
    "primaryColor": "#81B7B1",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#81B7B1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#81B7B1"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
